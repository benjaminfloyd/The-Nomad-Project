
<%= render :partial => 'partials/user' %>

<div class = 'center'>
<div id='user-show-posts', class='center'>
<div id='user-show-center'>
<div class="travel-feature-card-details">
<% current_user.posts.order(created_at: :desc).each do |post|%>
	<%= link_to(city_post_path(post.city_id, post.id)) do %>
    <div class="row">
      <div class="small-12 medium-12 columns travel-feature-card-content">
        <div class="row, center">
          <div class="small-12 medium-12 columns">
           <%= image_tag post.img, :class => 'user-post-image' %>
          </div>
          <div class="small-12 medium-12 columns">
            <h6 class="travel-feature-card-title"><%= post.title %></h6>
						<p class='post-date'>Posted: <%= post.updated_at %></p>
						<div class="user-show-post">
            <p><%= post.post %></p>
						</div>
          </div>
        </div> 
      </div>
			<% end %>
			<% end %>
		</div>
</div>


